{
  "compilerOptions": {
    "module": "es2015",
    "target": "es5",
    "noImplicitAny": false,
    "sourceMap": false,
    "jsx": "react",
    "allowJs": true,
    "moduleResolution": "node",
    "downlevelIteration": true
  },
  "exclude": [
    "node_modules"
  ],
  "experimentalDecorators": true,
  "emitDecoratorMetadata": true
}
